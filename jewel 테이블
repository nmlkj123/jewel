--회원 번호--
CREATE SEQUENCE MEM_NUM_SEQ
 START WITH 0
 INCREMENT BY 1
 NOMAXVALUE
 MINVALUE 0
NOCYCLE;

--상품 번호--
CREATE SEQUENCE ITEM_NUM_SEQ
 START WITH 0
 INCREMENT BY 1
 NOMAXVALUE
 MINVALUE 0
NOCYCLE;

